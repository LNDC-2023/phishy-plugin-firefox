{
  "manifest_version": 3,
  "name": "phishy",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage", "webRequest", "tabs"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["src/injector.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "phishy@minomy13.de"
    }
  }
}